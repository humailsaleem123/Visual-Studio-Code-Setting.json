{
  "workbench.iconTheme": "helium-icon-theme",
  "editor.fontSize": 16,
  "editor.fontFamily": "'Cascadia Code', 'Fira Code'",
  "editor.lineHeight": 25,
  "editor.autoIndent": "full",
  "editor.cursorSmoothCaretAnimation": "on",
  "editor.cursorStyle": "line",
  "editor.formatOnPaste": true,
  "editor.fontLigatures": true,
  "editor.formatOnType": false,
  "editor.formatOnSave": true,
  "editor.letterSpacing": 0.3,
  "editor.minimap.enabled": false,
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.suggestSelection": "first",
  "editor.tabCompletion": "on",
  "editor.tabSize": 5,
  "editor.wordWrap": "on",
  "files.autoSave": "afterDelay",
  "files.autoSaveDelay": 1000,
  "search.showLineNumbers": true,
  "workbench.colorCustomizations": {},
  "zenMode.centerLayout": false,
  "workbench.colorTheme": "Winter is Coming (Dark Blue)",
  "powermode.presets": "flames",
  "powermode.enabled": true,
  "powermode.combo.counterEnabled": "hide",
  "powermode.shake.intensity": 0,
  "editor.cursorBlinking": "smooth",
  "editor.cursorSurroundingLines": 15,
  "powermode.combo.location": "off",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "terminal.integrated.shellArgs.windows": ["-ExecutionPolicy", "Bypass"],
  "git.openRepositoryInParentFolders": "never",
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "emulator.androidColdBoot": true,
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "helium-icon-theme.activeIconPack": "react",
  "helium-icon-theme.languages.associations": {},
  "helium-icon-theme.folders.associations": {
    "blocks": "meta",
    "ReduxStore": "redux-store",
    "slices": "redux-actions",
    "contexts": "pipe",
    "sample": "dist"
  }
}
